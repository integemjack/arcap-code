{
    "name": "arcap",
    "version": "3.8.180",
    "description": "",
    "main": "init.js",
    "scripts": {
        "test": "concurrently \"npm run start:hot\" \"npm run webpack\"",
        "start": "node init.js",
        "start:hot": "nodemon -e *.js init.js test",
        "jshint": "jshint -c .jshintrc .",
        "webpack": "webpack -w -p -d --colors --profile",
        "build": "npm run build:web && npm run build:pkg && npm run build:nsi",
        "build:web": "webpack -p --colors --profile",
        "prebuild:pkg": "node ./build/build.js",
        "build:pkg": "pkg . --out-dir ./build/out -t node12-win --debug",
        "build:nsi": "node ./build/setup.js",
        "sass": "node-sass static/css/designer-default.scss static/css/designer-default.css",
        "test-recorder": "electron ./electron/app.js",
        "sync": "node ./command/syncData.js",
        "sync:server": "node ./command/syncData.js server",
        "python:install": "wget -c https://www.python.org/ftp/python/3.6.5/python-3.6.5-amd64.exe && /python-3.6.5-amd64.exe",
        "speech:install": "pip install deepspeech-gpu && pip install deepspeech-server",
        "speech:download": "wget -c https://github.com/mozilla/DeepSpeech/releases/download/v0.1.1/deepspeech-0.1.1-models.tar.gz && tar zxvf deepspeech-0.1.1-models.tar.gz",
        "speech:server": "deepspeech-server --config ./config/speech.json",
        "version": "node package.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run version && git add package.json && pretty-quick --staged"
        }
    },
    "bin": {
        "main": "init.js"
    },
    "pkg": {
        "scripts": [
            "arapp/**/*.js",
            "command/**/*.js",
            "libs/**/*.js",
            "loginapp/**/*.js",
            "models/**/*.js",
            "qrcodeapp/**/*.js",
            "socketio/**/*.js"
        ],
        "assets": [
            "cert/*",
            "arapp/views/*",
            "loginapp/views/*",
            "qrcodeapp/views/*",
            "static/**/*"
        ]
    },
    "repository": {
        "type": "git",
        "url": "http://99.109.54.153:8821/integem/Arcap.git"
    },
